{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/install.sh",
      "destination": "https://raw.githubusercontent.com/SudoDog-official/sudodog/main/install.sh"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/website/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/website/$1"
    }
  ]
}
